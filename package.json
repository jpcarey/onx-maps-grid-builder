{
  "name": "onx-maps-grid-builder",
  "version": "1.0.0",
  "description": "A tool to help generate a grid for offline maps in onX",
  "bin": "./build/index.js",
  "main": "./build/index.js",
  "engines": {
    "node": ">=18",
    "npm": ">=7"
  },
  "scripts": {
    "test": "jest --coverage --silent",
    "build": "tsc",
    "package": "npm run build && pkg ."
  },
  "author": "Jared Carey",
  "license": "ISC",
  "dependencies": {
    "@googlemaps/polyline-codec": "^1.0.28",
    "arg": "^5.0.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^18.4.8",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "targets": [
      "node18-linux-arm64",
      "node18-macos-arm64",
      "node18-win-arm64"
    ],
    "compress": "GZip",
    "outputPath": "dist"
  }
}
